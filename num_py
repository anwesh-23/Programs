import numpy as np

# Task 1: Create a NumPy array of integers from 1 to 10. Then, square each element in the array.
arr = np.arange(1, 11)
squared_arr = arr ** 2
print("Original array:", arr)
print("Squared array:", squared_arr)

# Task 2: Replace all elements greater than 5 in an array with 0.
arr[arr > 5] = 0
print("Array after replacing elements greater than 5 with 0:", arr)

# Task 3: Perform matrix multiplication of two 2x2 matrices.
matrix1 = np.array([[1, 2], [3, 4]])
matrix2 = np.array([[5, 6], [7, 8]])
result = np.dot(matrix1, matrix2)
print("Matrix 1:\n", matrix1)
print("Matrix 2:\n", matrix2)
print("Matrix multiplication result:\n", result)

# Task 4: Find the unique elements in a NumPy array.
unique_elements = np.unique(np.array([1, 2, 2, 3, 4, 4, 5]))
print("Unique elements in the array:", unique_elements)

# Task 5: Create a 2D array of shape (3, 4) with values from 1 to 12. Extract the first two rows.
array_2d = np.arange(1, 13).reshape(3, 4)
first_two_rows = array_2d[:2, :]
print("2D array:\n", array_2d)
print("First two rows:\n",Â first_two_rows)
